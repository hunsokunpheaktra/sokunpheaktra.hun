                <icons icon-field="Priority">
                    <icon value="1-ASAP">bullet_ball_glass_red.png</icon>
                    <icon value="2-High">bullet_ball_glass_red.png</icon>
                    <icon value="3-Medium">bullet_ball_glass_yellow.png</icon>
                    <icon value="4-Low">bullet_ball_glass_green.png</icon>
                </icons>
            </list-layout>
            <detail-layout>
                <page name="%KEY_INFO%">
                    <section name="%KEY_INFO%">
                        <field>SRNumber</field>
                        <field>Subject</field>
                        <field>Type</field>
                        <field>Priority</field>
                        <field>OpenedTime</field>
                        <field>Status</field>
                    </section>
                    <section name="%ADDITIONAL_INFO%">
                        <field>WorkPhone</field>
                        <field>Area</field>
                        <field>Cause</field>
                        <field>Source</field>
                    </section>
                </page>
            </detail-layout>
        </subtype>
        
